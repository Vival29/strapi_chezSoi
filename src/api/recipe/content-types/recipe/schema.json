{
  "kind": "collectionType",
  "collectionName": "recipes",
  "info": {
    "singularName": "recipe",
    "pluralName": "recipes",
    "displayName": "Recipe"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Ingredients": {
      "type": "json"
    },
    "RecipeName": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "ImageRecipe": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "saisons": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::saison.saison",
      "mappedBy": "recipes"
    }
  }
}
